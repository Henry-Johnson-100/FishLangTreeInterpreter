fish and
  >(x)>
  >(y)>
  <(
    fin
    >(x)>
    >(y)>
    >(False)>
  )<

fish or
  >(x)>
  >(y)>
  <(
    fin
    >(x)>
    >(True)>
    >(y)>
  )<

fish factorial
  >(n)>
  >(
    fish fact_
      >(n)>
      >(prd)>
      swim
        >(new_n <(- >(n)> >(1)>)<)>
        >(new_prd <(* >(prd)> >(n)>)<)>
        <(
          fin
          >(<= >(n)> >(0)>)>
          >(prd)>
          >(fact_ >(new_n)> >(new_prd)>)>
        )<
  )>
  swim
  >(trout >("computing the factorial of: ")>)>
  >(trout >(n)>)>
  <(fact_ >(n)> >(1)>)<

swim
>(trout >("Running: ")>)>
>(trout >("")>)>
<(factorial >(30)>)<